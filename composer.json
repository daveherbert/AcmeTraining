{
    "name": "inviqa/acme-training",
    "type": "magento-module",
    "require": {
        "magento-hackathon/magento-composer-installer": "^3.0"
    },
    "extra": {
        "magento-root-dir": "./public",
        "deploy-strategy": "symlink",
        "map": [
            ["app/etc/modules/Acme_Training.xml", "app/etc/modules/"],
            ["app/code/community/Acme/*", "app/code/community/Acme/"],
            ["app/design/frontend/base/default/layout/training.xml", "app/design/frontend/base/default/layout/"],
            ["app/design/frontend/base/default/template/training/*", "app/design/frontend/base/default/template/training/"]
        ]
    }
}
